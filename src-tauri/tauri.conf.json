{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "go-abacus",
  "version": "0.2.0",
  "identifier": "com.go-abacus.app",
  "build": {
    "beforeBuildCommand": "bun run build",
    "beforeDevCommand": "bun run dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "app": {
    "windows": [
      {
        "height": 600,
        "title": "go-abacus v0.2.0",
        "width": 900
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {}
}